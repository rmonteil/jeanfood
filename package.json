{
  "name": "jeanfood",
  "version": "0.0.1",
  "description": "A bot which tell you where to eat today",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w & nodemon ./public/index.js",
    "start": "node ./public/index.js",
    "lint": "tslint -p ./tsconfig.json",
    "test": "echo \"no tests for now\""
  },
  "nodemonConfig": {
    "watch": [
      "public/"
    ],
    "ignore": [
      "test/*",
      "docs/*"
    ],
    "delay": "500",
    "env": {
      "NODE_ENV": "develop",
      "NODE_PORT": 3000
    },
    "ext": "js json"
  },
  "keywords": [
    "jeanfood",
    "jean-food",
    "jean food",
    "mattermost",
    "bot",
    "lunch",
    "dinner",
    "location",
    "food"
  ],
  "author": "Robin Monteil",
  "license": "MIT",
  "dependencies": {
    "body-parser": "^1.18.2",
    "express": "^4.11.1",
    "morgan": "^1.9.0",
    "node-fetch": "^1.7.3",
    "node-mattermost": "0.0.1",
    "node-schedule": "^1.3.0",
    "winston": "^2.4.0"
  },
  "devDependencies": {
    "@types/debug": "0.0.30",
    "@types/express": "^4.11.1",
    "@types/express-serve-static-core": "^4.11.1",
    "@types/morgan": "^1.7.35",
    "@types/node-fetch": "^1.6.7",
    "@types/node-schedule": "^1.2.2",
    "@types/request": "^2.47.0",
    "debug": "^3.1.0",
    "i": "^0.3.6",
    "nodemon": "^1.14.12",
    "ts-node": "^4.1.0",
    "tslint": "^5.9.1",
    "typescript": "^2.7.1"
  }
}
